# Encryption key for file security (generate a secure random key in production)
# Generate with: openssl rand -hex 32
ENCRYPTION_KEY=your-super-secret-encryption-key-here-make-it-at-least-32-characters-long

# File upload settings
MAX_FILE_SIZE=104857600
UPLOAD_EXPIRATION_HOURS=24

# Rate limiting
RATE_LIMIT_WINDOW_MS=3600000
MAX_UPLOADS_PER_HOUR=10
MAX_DOWNLOADS_PER_HOUR=50

# Node environment
NODE_ENV=development